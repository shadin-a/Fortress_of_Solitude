{
  "name": "fortress_of_solitude",
  "engines": {
    "node": "18.12.1"
  },
  "version": "1.0.1",
  "description": "An online board posting site where a community shares posts to advertise yard work to be done or people willing to do the yard work",
  "main": "server/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server/server.js",
    "develop": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "build": "cd client && CI='' npm run build",
    "seed": "cd server && node seeders/seed.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zshep/Fortress_of_Solitude.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zshep/Fortress_of_Solitude/issues"
  },
  "homepage": "https://gomode.herokuapp.com/",
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "apollo-server": "^3.11.1",
    "bcrypt": "^5.1.0",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "mongo": "^0.1.0",
    "mongoose": "^6.0.0"
  }
}